import java.util.Scanner;
public class Sample {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int count=0;
        while(n!=0){
            n/=10;
            count++;
        }
        System.out.println(count);
    }
}


Input : 

325345

Expected output:

6
