import java.util.Scanner;
public class Sample {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        for(int i=N;i>=1;i--){
            for(int j=i;j>=1;j--){
            System.out.print(j);
        }
        System.out.println();
        
    }
}
}

Input:

n = 4

Expected output:

4321

321

21

1

